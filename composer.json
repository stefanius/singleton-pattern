{
  "name": "stefanius/singleton-pattern",
  "description": "Helper package to implement a Singleton class.",
  "keywords": ["singleton", "framework", "agnostic", "symfony", "laravel", "zend", "stefanius", "design", "pattern"],
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "Stephan Grootveld",
      "email": "sgrootveld@gmail.com",
      "homepage": "http://www.stephangrootveld.nl",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "friendsofphp/php-cs-fixer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Stefanius\\SingletonPattern\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/SingletonTest.php",
      "tests/TestClass.php"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
